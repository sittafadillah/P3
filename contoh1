#include <iostream>
using namespace std;

int main () {
    int bilangan;
    int definisi_bilangan;

    cout<<"masukan bilangan :";
    cin>>bilangan;

    definisi_bilangan=bilangan%2;

    if(definisi_bilangan==0){
        cout<<"bilangan genap";
    }else{
        cout<<"bilangan ganjil";
    }

    return 0;
}
