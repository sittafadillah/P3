#include <iostream>
using namespace std;
int main()
{
    int Nilai_tes;
    int Nilai_uts;
    int Nilai_uas;
    int Nilai_tugas;
    int Nilai_akhir;

    cout << "Nilai_tes_Matematika: ";
    cin >> Nilai_tes;//membaca inputan dari keyboard

    cout<< "Nilai_uts_Matematika:";
    cin>>Nilai_uts;

    cout<< "Nilai_uas_Matematika:";
    cin>>Nilai_uas;

    cout<< "Nilai_tugas_Matematika:";
    cin>>Nilai_tugas;

    Nilai_akhir=0.20*Nilai_tes+0.30*Nilai_uts+0.30*Nilai_uas+0.20*Nilai_tugas;

    if(Nilai_akhir<=50){
        cout<<"Indeks D";
    }else if(Nilai_akhir>=50 && Nilai_akhir<=70){
        cout<<"Indeks C";
    }else if(Nilai_akhir>=70 && Nilai_akhir<=80){
        cout<<"Indeks B";
    }else{cout<<"Indeks A";}


    return 0;
}


